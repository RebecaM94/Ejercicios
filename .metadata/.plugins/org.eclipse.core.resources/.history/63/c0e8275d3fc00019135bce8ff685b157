#include "hal_data.h"
#include "FilterStateMachine.h"

int state = 0;
bool TriggerCnt = 0;

void FilterStateMachine()
{
    switch (state)
    {
        case 0:
            TriggerCnt = 0;
            state = 1;
            break;

        case 1:
            TriggerCnt = 0;

            break;
    }
}
